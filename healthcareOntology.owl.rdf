<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/mdiab1/ontologies/2024/11/untitled-ontology-5/"
     xml:base="http://www.semanticweb.org/mdiab1/ontologies/2024/11/untitled-ontology-5/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
